#include <iostream>
#include <algorithm>

using namespace std;

void quickSort(int arr[], int left, int right);

int main()
{

    int arr[10];
    for(int i = 0; i < 10; i++)
    {
        cin >> arr[i];
    }
    int l = 0;
    int r = 9;

    quickSort(arr, l, r);
    for(int i = 0; i < 10; i++)
    {
        cout << arr[i] << " ";
    }
}
void quickSort(int arr[], int left, int right)
{
    if(left <= right)
    {
        int i = left;
        int j = right;
        int key = arr[(i + j) / 2];
        while(i < j)
        {
            while(arr[i] <= key)
            {
                i++;
            }
            while(arr[j] > key)
            {
                j--;
            }
            if(i <= j)
            {
                swap(arr[i], arr[j]);
                i++;
                j--;
            }
        }
        if(left <= j)
        {
            quickSort(arr, left, j);
        }
        if(i < right)
        {
            quickSort(arr, i, right);
        }
    }
}
